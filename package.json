{
  "name": "smelte",
  "description": "UI framework for Svelte using Tailwind CSS",
  "version": "1.0.6",
  "scripts": {
    "dev": "sapper dev -p 7777",
    "build": "rollup -c --config rollup-publish.config.js",
    "export": "sapper export --legacy --timeout 30000",
    "lint": "prettier --write --plugin-search-dir=. ./src/**/*",
    "test": "ava"
  },
  "pre-commit": [
    "lint"
  ],
  "files": [
    "dist",
    "src/utils",
    "src/tailwind.css",
    "src/index.js",
    "src/breakpoints.js",
    "src/notification-queue.js",
    "src/components",
    "tailwind.config.js",
    "postcss.config.js",
    "src/dark.js",
    "rollup-plugin-smelte.js"
  ],
  "ava": {
    "require": [
      "esm"
    ]
  },
  "module": "dist/module.js",
  "main": "dist/index.js",
  "browserslist": "last 3 version",
  "svelte": "src/index.js",
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "autoprefixer": "^9.5.1",
    "cssnano": "^4.1.10",
    "postcss": "^7.0.16",
    "postcss-import": "^12.0.1",
    "postcss-input-range": "^4.0.0",
    "postcss-url": "^8.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "svelte": "^3.7.1",
    "svelte-preprocess": "^3.9.12",
    "svelte-waypoint": "^0.1.4",
    "tailwind-css-variables": "^2.0.3",
    "tailwindcss": "^1.2.0",
    "tailwindcss-elevation": "^0.3.0",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@now/node": "^0.9.0",
    "ava": "3.8.2",
    "compression": "^1.7.1",
    "eslint": "^6.0.1",
    "eslint-plugin-svelte3": "^2.1.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "prettier-plugin-svelte": "^0.6.0",
    "prismjs": "^1.16.0",
    "rollup": "^1.0.0",
    "rollup-plugin-alias": "^1.5.2",
    "rollup-plugin-analyzer": "^3.1.1",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.1.3",
    "sapper": "^0.27.8",
    "sirv": "^0.4.0"
  }
}
